.textContent = `Inst√¢ncia: ${name}`;\n\n      QRCode.toCanvas(canvas, data, {\n        width: 300, // Tamanho do QR Code\n        margin: 0,  // Remover bordas\n        color: {\n          dark: \"#000000\", // Preto para o QR Code\n          light: \"#FFFFFF\" // Branco para o fundo\n        }\n      }, function (error) {\n        if (error) {\n          console.error(\"Erro ao gerar QR Code:\", error);\n        } else {\n          console.log(\"QR Code gerado com sucesso.\");\n        }\n      });\n    }\n\n    // Gera o QR Code automaticamente ao carregar a p√°gina\n    window.onload = generateQRCode;\n  </script>\n</body>\n</html>\n"
